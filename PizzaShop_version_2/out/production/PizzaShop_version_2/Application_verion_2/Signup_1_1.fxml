<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<AnchorPane maxHeight="400.0" maxWidth="400.0" minHeight="405.0" minWidth="400.0" prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Application_verion_2.SignupController">
   <children>
      <VBox layoutX="60.0" layoutY="101.0" prefHeight="244.0" prefWidth="300.0" spacing="10.0">
         <children>
            <Label text="Username" />
            <TextField fx:id="usernameField" prefHeight="25.0" prefWidth="299.0" promptText="Username" />
            <Label text="Password" />
            <TextField fx:id="passwordField" promptText="Password" />
            <Button fx:id="signup_btn" mnemonicParsing="false" onAction="#AddNewUser" text="Sign up" />
            <Button fx:id="cancel_btn" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
         </children>
      </VBox>
      <Label fx:id="Signup_lbl" layoutX="38.0" layoutY="14.0" text="Sign Up">
         <font>
            <Font size="35.0" />
         </font>
      </Label>
      <Label fx:id="status_lbl" layoutX="60.0" layoutY="74.0" />
   </children>
</AnchorPane>
