<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Application.DelieveryController">
   <children>
      <GridPane alignment="CENTER" layoutX="70.0" layoutY="14.0" prefHeight="312.0" prefWidth="461.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="130.0" minWidth="10.0" prefWidth="62.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="207.0" minWidth="10.0" prefWidth="207.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints maxHeight="82.0" minHeight="10.0" prefHeight="25.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="82.0" minHeight="6.0" prefHeight="29.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="184.0" minHeight="10.0" prefHeight="172.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="29.0" minHeight="10.0" prefHeight="29.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="next_btn" mnemonicParsing="false" onAction="#next" prefHeight="44.0" prefWidth="85.0" text="Next" GridPane.columnIndex="2" GridPane.rowIndex="3" />
            <VBox prefHeight="200.0" prefWidth="242.0" spacing="10.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
               <children>
                  <TextField fx:id="address1_txt" promptText="Address Line 1" />
                  <TextField fx:id="address2_txt" promptText="Address Line 2" />
                  <TextField fx:id="city_txt" promptText="City" />
                  <HBox prefHeight="17.0" prefWidth="186.0" spacing="50.0">
                     <children>
                        <TextField fx:id="state_txt" layoutX="10.0" layoutY="10.0" promptText="State" />
                        <TextField fx:id="zipCode_txt" promptText="Zip Code" />
                     </children>
                  </HBox>
                  <TextField fx:id="phoneNum_txt" promptText="Phone Number" />
               </children>
            </VBox>
            <Label contentDisplay="CENTER" prefHeight="31.0" prefWidth="100.0" text="Delivery" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label contentDisplay="CENTER" prefHeight="44.0" prefWidth="131.0" text="Assign Order" GridPane.columnIndex="1">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Button fx:id="back_btn" layoutX="325.0" layoutY="293.0" mnemonicParsing="false" onAction="#back" prefHeight="44.0" prefWidth="85.0" text="Back" GridPane.rowIndex="3" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
